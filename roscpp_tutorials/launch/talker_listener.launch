<launch>
  <arg name="machine" default="aero_v"/>

  <include file="$(find jsk_aero_startup)/aero.machine"/>

  <node name="listener" pkg="roscpp_tutorials"
        type="listener"
        output="screen"
        machine="$(arg machine)"/>
  <node name="talker" pkg="roscpp_tutorials"
        type="talker"
        output="screen"
        machine="$(arg machine)"/>
</launch>
